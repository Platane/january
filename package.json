{
  "name": "pizza-tracker-app",
  "version": "1.0.0",
  "dependencies": {
    "babel-register": " 6.24.1",
    "md5": " 2.2.1",
    "prettier": " 1.3.1",
    "react": " 15.5.4",
    "react-dom": " 15.5.4",
    "react-redux": " 5.0.4",
    "redux": " 3.6.0"
  },
  "devDependencies": {
    "autoprefixer": " 6.7.7",
    "babel-cli": " 6.24.1",
    "babel-core": " 6.24.1",
    "babel-loader": " 7.0.0",
    "babel-plugin-css-modules-transform": " 1.2.7",
    "babel-plugin-transform-class-properties": " 6.24.1",
    "babel-preset-es2015": " 6.24.1",
    "babel-preset-es2017": " 6.24.1",
    "babel-preset-flow": " 6.23.0",
    "babel-preset-react": " 6.24.1",
    "babel-preset-stage-1": " 6.24.1",
    "css-loader": " 0.28.1",
    "extract-text-webpack-plugin": " 2.1.0",
    "file-loader": " 0.11.1",
    "flow-bin": "0.45.0",
    "flow-typed": " 2.1.2",
    "html-loader": " 0.4.5",
    "html-minify-loader": " 1.1.0",
    "markdown-it": " 8.3.1",
    "postcss-import": " 9.1.0",
    "postcss-loader": " 1.3.3",
    "postcss-simple-vars": " 3.1.0",
    "raw-loader": " 0.5.1",
    "uglifyjs-webpack-plugin": " 0.4.3",
    "webpack": " 2.5.0",
    "webpack-dev-server": " 2.4.5"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --devtool source-map -d",
    "serve:dist": "node scripts/server.js",
    "build:asset": "env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:contentData": "node scripts/mkdir.js dist && node scripts/contentParser/cmd.js src/content/post > dist/posts.json",
    "build:pages": "env NODE_ENV=server node scripts/pageRenderer/cmd.js dist webpack-stats.json",
    "build:images": "node scripts/imageBundler/cmd.js src/content/post dist",
    "build": "yarn run build:contentData && yarn run build:images && yarn run build:asset && yarn run build:pages",
    "prettier:test": "prettier --single-quote --trailing-comma es5 --no-semi --tab-width 4 -l \"src/**/*.js\"",
    "prettier:write": "prettier --single-quote --trailing-comma es5 --no-semi --tab-width 4 --write \"src/**/*.js\"",
    "flow-typed": "flow-typed install",
    "test": "yarn run prettier:test"
  }
}
