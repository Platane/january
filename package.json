{
  "name": "january",
  "version": "1.0.0",
  "dependencies": {
    "babel-polyfill": " 6.23.0",
    "prop-types": " 15.5.10",
    "react": " 15.5.4",
    "react-dom": " 15.5.4",
    "react-propstransition": " 2.0.4",
    "react-redux": " 5.0.5",
    "react-swipeable-views": " 0.12.3",
    "redux": " 3.6.0",
    "whatwg-fetch": " 2.0.3"
  },
  "devDependencies": {
    "autoprefixer": " 7.1.1",
    "babel-cli": " 6.24.1",
    "babel-core": " 6.24.1",
    "babel-loader": " 7.0.0",
    "babel-plugin-css-modules-transform": " 1.2.7",
    "babel-plugin-transform-class-properties": " 6.24.1",
    "babel-plugin-transform-string-literal-replace": " 1.0.2",
    "babel-preset-env": " 1.5.2",
    "babel-preset-es2015": " 6.24.1",
    "babel-preset-es2017": " 6.24.1",
    "babel-preset-flow": " 6.23.0",
    "babel-preset-react": " 6.24.1",
    "babel-preset-stage-1": " 6.24.1",
    "babel-register": " 6.24.1",
    "css-loader": " 0.28.4",
    "enzyme": " 2.8.2",
    "extract-text-webpack-plugin": " 2.1.0",
    "file-loader": " 0.11.2",
    "flow-bin": " 0.47.0",
    "flow-typed": " 2.1.2",
    "html-loader": " 0.4.5",
    "html-minify-loader": " 1.1.0",
    "jest": " 20.0.4",
    "markdown-it": " 8.3.1",
    "md5": " 2.2.1",
    "optimize-css-assets-webpack-plugin": " 2.0.0",
    "postcss-import": " 10.0.0",
    "postcss-loader": " 2.0.5",
    "postcss-simple-vars": " 4.0.0",
    "prettier": " 1.4.2",
    "raw-loader": " 0.5.1",
    "react-test-renderer": " 15.5.4",
    "smartcrop-gm": " 1.0.2",
    "uglifyjs-webpack-plugin": " 0.4.3",
    "webpack": " 2.6.1",
    "webpack-dev-server": " 2.4.5"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --devtool source-map -d",
    "serve:dist": "node scripts/server.js",
    "build:asset": "env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:contentData": "node scripts/mkdir.js dist && node scripts/contentParser/cmd.js src/content/post > dist/posts.json",
    "build:pages": "env NODE_ENV=server node scripts/pageRenderer/cmd.js dist webpack-stats.json",
    "build:data": "node scripts/dataRenderer/cmd.js dist",
    "build:images": "node scripts/imageBundler/cmd.js src/content/post dist",
    "build:icons": "node scripts/iconBundler/cmd.js src/asset/icon dist",
    "build:serviceWorker": "node scripts/buildServiceWorker.js dist webpack-stats.json",
    "build:manifest": "node scripts/buildManifest.js dist",
    "build:rss": "node scripts/buildRss.js dist",
    "build": "npm run build:contentData && npm run build:images && npm run build:icons && npm run build:asset && npm run build:data && npm run build:pages && npm run build:serviceWorker && npm run build:manifest && npm run build:rss",
    "build:forDev": "(rm -r dist || echo 1) && npm run build:contentData && npm run build:images && npm run build:data",
    "prettier:test": "prettier --single-quote --trailing-comma es5 --no-semi --tab-width 4 -l \"**/*.js\"",
    "prettier:write": "prettier --single-quote --trailing-comma es5 --no-semi --tab-width 4 --write \"**/*.js\"",
    "flow-typed": "flow-typed install",
    "test:unit": "env NODE_ENV=test jest --verbose",
    "test": "npm run test:unit && npm run prettier:test"
  }
}
